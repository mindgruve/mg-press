{% extends 'style-guide/layout/element-dynamic.twig' %}

{% set template = _self %}
{% set container = true %}
{% set headline = 'Arrows' %}

{# Markup to render here #}
{% set element_content %}
    <a href="#" class="btn btn-primary" role="button">Register Now</a>
    <a href="#" class="btn btn-primary active" role="button">Register Now</a>
    <a href="#" class="btn btn-primary hover" role="button">Register Now</a>
{% endset %}


{# Override Code Display Here - defaults to `element_content` #}
{% set code %}
<a href="#" class="btn btn-primary" role="button">Register Now</a>
{% endset %}


{# Add Notes Here #}
{% set notes %}

{% endset %}